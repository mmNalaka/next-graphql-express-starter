{
  "name": "empress",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "format": "prettier --write",
    "genrate:prisma": "yarn prisma generate",
    "generate:nexus": "ts-node --project tsconfig.server.json --transpile-only server/graphql/schema",
    "postinstall": "yarn genrate:prisma && yarn generate:nexus",
    "studio": "yarn prisma studio"
  },
  "dependencies": {
    "@prisma/client": "2.21.2",
    "apollo-server-express": "^2.23.0",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-middleware": "^6.0.8",
    "graphql-shield": "^7.5.0",
    "next": "10.1.3",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.34.1",
    "passport": "^0.4.1",
    "passport-google-oauth2": "^0.2.0",
    "postcss": "^8.2.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^2.1.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/node": "^15.0.1",
    "@types/passport": "^1.0.6",
    "@types/passport-google-oauth2": "^0.1.3",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "prisma": "^2.21.2",
    "ts-node": "^9.1.1",
    "typescript": "4.2.4"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  }
}
